platform :ios, '8.0'

use_frameworks!


target 'iOSWheels' do
    
inhibit_all_warnings! #屏蔽所有warning

    
pod 'AFNetworking'
pod 'FMDB'
pod 'MJExtension'
#下拉刷新
pod 'MJRefresh'


pod 'ReactiveCocoa', '~> 2.5'
pod 'Masonry'



pod 'YYText'
pod 'YYCache'

# 对象容错处理
pod 'SafeObject'

# 监控内容泄漏
pod 'MLeaksFinder'


end

